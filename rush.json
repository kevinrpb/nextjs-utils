{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.75.0",
  "pnpmVersion": "6.7.1",
  "pnpmOptions": {
    "strictPeerDependencies": true,
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0 <17.0.0",
  "ensureConsistentVersions": true,
  "gitPolicy": {},
  "repository": {
    "url": "https://github.com/kevinrpb/nextjs-utils",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "@nextjs-utils/dynamic-icon",
      "projectFolder": "components/dynamic-icon",
      "versionPolicyName": "nextjs-utils"
    },
    {
      "packageName": "@nextjs-utils/sane-link",
      "projectFolder": "components/sane-link",
      "versionPolicyName": "nextjs-utils"
    }
  ]
}
